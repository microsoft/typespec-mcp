{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "jsx": "preserve",
    "emitDeclarationOnly": true,
    "jsxImportSource": "@alloy-js/core",
    "outDir": "./dist"
  },
  "include": [
    "src/**/*.ts",
    "src/**/*.tsx",
    "testing/**/*.ts",
    "test/**/*.ts",
    "test/**/*.tsx"
  ],
  "exclude": ["node_modules", "dist"]
}
