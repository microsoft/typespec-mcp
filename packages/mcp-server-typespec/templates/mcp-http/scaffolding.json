{
  "mcp on http": {
    "title": "MCP Server on Http Service Definition",
    "description": "Create a project representing an MCP server on Http Service API.",
    "compilerVersion": "1.0.0-rc.0",
    "libraries": [
      {
        "name": "typespec-mcp",
        "version": "workspace:^"
      }
    ],
    "emitters": {
      "typespec-mcp-server-rest-js": {
        "version": "workspace:^",
        "selected": true,
        "label": "MCP server on Http emitter",
        "options": {}
      }
    },
    "files": [
      {
        "path": "mcp-http/main.tsp",
        "destination": "main.tsp"
      },
      {
        "path": "mcp-http/mcp.tsp",
        "destination": "mcp.tsp"
      },
      {
        "path": "mcp-http/package.json",
        "destination": "package.json"
      },
      {
        "path": "mcp-http/tsconfig.json",
        "destination": "tsconfig.json"
      },
      {
        "path": "mcp-http/src/index.ts",
        "destination": "src/index.ts"
      },
      {
        "path": "mcp-http/.vscode/mcp.json",
        "destination": ".vscode/mcp.json"
      }
    ]
  }
}
